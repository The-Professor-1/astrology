{
    "version": 2,
    "builds": [
        {
            "src": "astrology/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "astrology/wsgi.py"
        }
    ],
    "build": {
        "command": "python manage.py collectstatic --noinput"
    }
}