{
    "version": 2,
    "builds": [
      {
        "src": "astrology/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "astrology/wsgi.py"
      }
    ],
    "env": {
      "DB_NAME": "@db_name",
      "DB_USER": "@db_user",
      "DB_PASSWORD": "@db_password",
      "DB_HOST": "@db_host",
      "DB_PORT": "5432"
    }
  }
  
  