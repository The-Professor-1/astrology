{
    "version": 2,
    "builds": [
      {
        "src": "astrology/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "astrology/wsgi.py"
      }
    ],
    "env": {
  "DB_NAME": "@DB_NAME",
  "DB_USER": "@DB_USER",
  "DB_PASSWORD": "@DB_PASSWORD",
  "DB_HOST": "@DB_HOST",
  "DB_PORT": "5432"
}
  }
  
  